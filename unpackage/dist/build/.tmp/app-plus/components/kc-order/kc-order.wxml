<block wx:if="{{data}}"><view class="kc_order data-v-06e4daae"><view class="kc_order_head data-v-06e4daae">{{data.type==100?'饿了么订单':'美团订单'}}</view><view class="kc_order_line data-v-06e4daae"></view><view class="kc_order_content data-v-06e4daae"><view class="kc_order_time_tag data-v-06e4daae">{{"创建时间: "+data.eleme_order.ctime}}</view><view class="kc_order_merchandise data-v-06e4daae"><view class="corner_mark data-v-06e4daae"></view><view class="kc_order_merchandise_head data-v-06e4daae">商品</view></view><block wx:for="{{data.eleme_order.Merchandise}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="kc_order_food data-v-06e4daae"><text class="data-v-06e4daae">{{"套餐名称: "+item.title}}</text><text style="margin-left:40px;" class="data-v-06e4daae">{{"SKU: "+item.sku}}</text><text style="float:right;" class="data-v-06e4daae">{{item.price+" ¥"}}</text><block wx:for="{{item.food}}" wx:for-item="foods" wx:for-index="indexs" wx:key="indexs"><view class="kc_order_food_detail data-v-06e4daae"><block wx:if="{{indexs===0}}"><view class="data-v-06e4daae"><text class="td data-v-06e4daae">名称</text><text class="td data-v-06e4daae">成本</text><text style="float:right;margin-right:30rpx;" class="data-v-06e4daae">单价</text></view></block><view class="data-v-06e4daae"><text class="td data-v-06e4daae">{{foods.name}}</text><text class="td data-v-06e4daae">{{foods.cost+" ¥"}}</text><text style="float:right;margin-right:30rpx;" class="data-v-06e4daae">{{foods.price+" ¥"}}</text></view></view></block></view></block><view style="float:right;font-size:24rpx;margin-top:20rpx;" class="data-v-06e4daae">{{'本单商家总利润: '+data.real_price+' ¥'}}</view></view></view></block>