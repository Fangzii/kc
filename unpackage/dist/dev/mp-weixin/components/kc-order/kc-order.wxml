<block wx:if="{{data}}"><view class="kc_order data-v-ed1b3cce"><view class="kc_order_head data-v-ed1b3cce">{{data.type==100?'饿了么订单':'美团订单'}}</view><view class="kc_order_line data-v-ed1b3cce"></view><view class="kc_order_content data-v-ed1b3cce"><view class="kc_order_time_tag data-v-ed1b3cce">{{"创建时间: "+data.eleme_order.ctime}}</view><view class="kc_order_merchandise data-v-ed1b3cce"><view class="corner_mark data-v-ed1b3cce"></view><view class="kc_order_merchandise_head data-v-ed1b3cce">商品</view></view><block wx:for="{{data.eleme_order.Merchandise}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="kc_order_food data-v-ed1b3cce"><text class="data-v-ed1b3cce">{{"套餐名称: "+item.title}}</text><text style="margin-left:40px;" class="data-v-ed1b3cce">{{"SKU: "+item.sku}}</text><text style="float:right;" class="data-v-ed1b3cce">{{item.price+" ¥"}}</text><block wx:for="{{item.food}}" wx:for-item="foods" wx:for-index="indexs" wx:key="indexs"><view class="kc_order_food_detail data-v-ed1b3cce"><block wx:if="{{indexs===0}}"><view class="data-v-ed1b3cce"><text class="td data-v-ed1b3cce">名称</text><text class="td data-v-ed1b3cce">成本</text><text style="float:right;margin-right:30rpx;" class="data-v-ed1b3cce">单价</text></view></block><view class="data-v-ed1b3cce"><text class="td data-v-ed1b3cce">{{foods.name}}</text><text class="td data-v-ed1b3cce">{{foods.cost+" ¥"}}</text><text style="float:right;margin-right:30rpx;" class="data-v-ed1b3cce">{{foods.price+" ¥"}}</text></view></view></block></view></block><view style="float:right;font-size:24rpx;margin-top:20rpx;" class="data-v-ed1b3cce">{{'本单商家总利润: '+data.real_price+' ¥'}}</view></view></view></block>